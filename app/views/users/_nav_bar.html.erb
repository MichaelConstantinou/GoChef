<header>
  <div id="nav-bar">
    <%= image_tag("original_logo.png") %>
    <h1>Go Chef</h1>
    <div class="nav-links">
      <% if current_user %>
         <div id="logout"><%= link_to "Logout", destroy_user_session_path, method: :delete %></div>
