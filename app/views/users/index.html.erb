<h2> Chefs: </h2>
<table class="chefs">
  <tr>
    <th>Name</th>
    <th>Location</th>
  </tr>
  <% @users.each do |chef|%>
    <tr>
      <td class="chef-name"><%= link_to "#{chef.name}", '/'%></td>
      <td>   <%="#{chef.location_lat} | #{chef.location_lon}"%></td>
    </tr>
  <% end %>
</table>
