<div class="chef-view">
   <h1><%= @user.name %></h1>
    <p class="bio"> <%= @user.bio %> </p>
      <div class="portrait" style="background-image:url(<%= image_path(@user.avatar) %>)"></div>
<h1>Your skillsets</h1>
<div id="memberships">
  <ul>
    <% @user.cuisines.each do |cuisine| %>
      <li><%= cuisine.food %></li>
    <% end %>
  </ul>
</div>
<div id="gallery">
  <%= image_tag("basil.png") %>
  <%= image_tag("fawlty-towers-rex.jpg") %>
  <%= image_tag("manuel.jpg") %>
  <%= image_tag("goosestep.jpg") %>
</div>

